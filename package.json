{
  "name": "questionnaire",
  "version": "0.1.0",
  "author": "Vasylyshyn Vitaliia <vasylyshynvitaliia@gmail.com>",
  "private": true,
  "scripts": {
    "start:dev": "next dev",
    "start:prod": "next build && next start",
    "build": "next build",
    "start": "next start",
    "lint": "next lint .",
    "format:check": "prettier --check .",
    "format:fix": "prettier --write .",
    "type:check": "tsc-files --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "next": "14.2.4",
    "react": "^18",
    "react-dom": "^18",
    "react-redux": "^9.1.2"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.1.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "sass": "^1.77.6",
    "tsc-files": "^1.1.4",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css}": "npm run format:fix",
    "*.{js,jsx,ts,tsx}": [
      "npm run lint",
      "npm run type:check"
    ]
  }
}
